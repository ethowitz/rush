exp           := literal
               | (if exp exp exp)
               | :\variable-name
def           := (let \variable-name exp)
literal       := SYM | NUM | BOOL
variable-name := :^[a-z_]\\w*$
SYM           := "^[\w\-. ]+$"
NUM           := sequence of digits 0-9 (signed)
BOOL          := true | false
